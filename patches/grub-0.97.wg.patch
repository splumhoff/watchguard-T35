+unsigned char LCM_GetButtons()
+{
+    unsigned char btn;
+    btn = inb(StatusPort);
+    if (LCDB_filton == 1) {	/* so, filton, do it filton way */
+        switch (btn)
+        {
+            /* down button */
+            case 0xC7:
+                return BTN_DOWN;
+            /* left */
+            case 0xE7:
+                return BTN_LEFT;
+            /* up */
+            case 0xCF:
+                return BTN_UP;
+            /* right */
+            case 0xEF:
+                return BTN_RIGHT;
+            default:
+                return 0;
+        }
+    } else { 			/* all other plcm based platfroms <vashon/kennewick> */
+        switch (btn)
+        {
+            /* down button */
+            case 0xC7:
+                return BTN_DOWN;
+            /* left */
+            case 0xCF:
+                return BTN_LEFT;
+            /* up */
+            case 0xE7:
+                return BTN_UP;
+            /* right */
+            case 0xEF:
+                return BTN_RIGHT;
+            default:
+                return 0;
+        }
+    }
+}
